<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!---Colores-->
    <Color x:Key="Color1">#121926</Color>
    <Color x:Key="Color2">#171F28</Color>
    <Color x:Key="Color3">#FAFAFA</Color>
    <Color x:Key="Color4">#FAFAFA</Color>
    <Color x:Key="Color5">#3A3A4A</Color>

    <Color x:Key="Color6">#3F4A72</Color>
    <Color x:Key="Color7">#F5F4F6</Color>
    <Color x:Key="Color8">#B0A7A1</Color>
    <Color x:Key="Color9">#0D131E</Color>

    <Color x:Key="Color10">#C17E54</Color>
    <Color x:Key="Color11">#7A7E82</Color>
    <Color x:Key="Color12">#202525</Color>

    <Color x:Key="Color13">#2E3746</Color>

    <!--Paleta-->
    
    <SolidColorBrush x:Key="PrimaryDarkColor" Color="{StaticResource Color1}"/>
    <SolidColorBrush x:Key="IntermediateColor" Color="{StaticResource Color2}"/>
    <SolidColorBrush x:Key="ColorFondo" Color="{StaticResource Color3}"/>
    <SolidColorBrush x:Key="PrimaryGrayColor" Color="{StaticResource Color5}"/>
    
    <!---color botones-->
    <SolidColorBrush x:Key="HoverGrayColor" Color="{StaticResource Color6}"/>
    <SolidColorBrush x:Key="PrimaryTextColor" Color="{StaticResource Color9}"/>        <!--Color de texto de botones y del titulo-->
    <SolidColorBrush x:Key="SecundaryTextColor" Color="{StaticResource Color7}"/>      <!--Color textos en oscuro siempre-->
    <SolidColorBrush x:Key="PrimaryTextColorDark_Light" Color="{StaticResource Color8}"/> <!---color hover resumenes y home2-->
    
    <!--otr.-->
    <SolidColorBrush x:Key="AccentColor" Color="{StaticResource Color10}"/>
    <SolidColorBrush x:Key="ColorFondoLista" Color="{StaticResource Color11}"/>
    <SolidColorBrush x:Key="ColorPanelDerecho" Color="{StaticResource Color12}"/>


    <SolidColorBrush x:Key="ColorPanelIntermedio" Color="{StaticResource Color12}"/>
    <SolidColorBrush x:Key="ColorBotonesPanelDerecho" Color="{StaticResource Color13}"/>

</ResourceDictionary>